<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yumehub.proto" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YUMEHUB</name>
    <description>
        Yumehub prototype
    </description>
    <author email="toni.bagur@coneptum.com" href="http://www.coneptum.com">
        Coneptum S.L.
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />

          <icon src="res/ios/S98_icon0.png" width="180" height="180" />

          <icon src="res/ios/s98_icon2.png" width="120" height="120" />

          <icon src="res/ios/s98_icon1.png" width="152" height="152" />
          <!-- iOS 6.1 -->
          <!-- Spotlight Icon -->


        <splash src="res/ios/s99_splash0.png" width="640" height="1136"/>
        <splash src="res/ios/s99_splash1.png" width="750" height="1334"/>
        <splash src="res/ios/s99_splash2.png" width="1242" height="2208"/>
        <splash src="res/ios/s99_splash3.png" width="640" height="960"/>
    </platform>
<preference name="EnableViewportScale" value="true"/>
</widget>
